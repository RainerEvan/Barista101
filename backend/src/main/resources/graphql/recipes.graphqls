type Recipes{
    id:ID!
    category:RecipeCategories
    author:Accounts
    title:String
    body:String
    thumbnail:String
    createdAt:DateTime
    updatedAt:DateTime
}

type RecipeCategories{
    id:ID!
    name:String
}

extend type Query{
    getAllRecipes:[Recipes]
    getRecipe(recipeId:ID!):Recipes
    getAllRecipesForAccount(accountId:ID!):[Recipes]
}