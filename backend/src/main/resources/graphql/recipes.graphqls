type Recipes{
    id:ID!
    category:RecipeCategories
    author:Accounts
    title:String
    description:String
    ingredients:String
    instructions:String
    notes:String
    createdAt:DateTime
}

type RecipeCategories{
    id:ID!
    name:String
}

extend type Query{
    getAllRecipes:[Recipes]
    getRecipe(recipeId:ID!):Recipes
    getAllRecipesForAccount(accountId:ID!):[Recipes]
}